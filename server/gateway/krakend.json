      "backend": [
        {
          "url_pattern": "/{id}",
          "method": "PATCH",
          "encoding": "json",
          "host": ["http://localhost:3001"],
          "extra_config": {
            "backend/http": {
              "return_error_code": true
            }
          }
        }
      ]
    },
    {
      "endpoint": "/user/{id}",
      "method": "DELETE",
      "output_encoding": "json",
      "backend": [
        {
          "url_pattern": "/{id}",
          "method": "DELETE",
          "encoding": "json",
          "host": ["http://localhost:3001"],
          "extra_config": {
            "backend/http": {
              "return_error_code": true
            }
          }
        }
      ]
    }
  ]
}
